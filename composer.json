{
  "name": "macellan/onesignal",
  "description": "OneSignal notifications channel for Laravel",
  "homepage": "http://macellan.net/",
  "license": "MIT",
  "authors": [
    {
      "name": "Fatih Aytekin",
      "email": "fatih@aytekin.me",
      "homepage": "https://github.com/faytekin",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.2|^7.0.1",
    "illuminate/notifications": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
    "illuminate/http": "^8.0|^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "6.24|^7.1|^8.0|^9.0",
    "mockery/mockery": "^1.4.4",
    "phpunit/phpunit": "^9.5.10|^10.0",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Macellan\\OneSignal\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Macellan\\OneSignal\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "test:coverage": "vendor/bin/phpunit --coverage-text --coverage-clover=coverage.clover"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Macellan\\OneSignal\\OneSignalServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
